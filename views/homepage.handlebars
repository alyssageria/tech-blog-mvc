{{#each posts as |post|}}
<div>
    <div class="card">
        <div class="card-header">
            <p class="card-header-title">{{{post.title}}}</p>
            <p class="card-header-subtitle">Posted by {{{user.username}}}</p>
        </div>
        <div class="card-content">
            <p>{{{post.post_content}}}</p>
        </div>
    </div>
</div>