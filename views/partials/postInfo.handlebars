<article class="post">
    <div class="card">
        <div class="card-header">
            <p class="card-header-title">{{title}}</p>
            <p class="card-header-subtitle">Posted by {{user.username}}</p>
        </div>
        <div class="card-content">
            <p>{{post_content}}</p>
        </div>
    </div>
</article>